@{
    ViewData["Title"] = "Subject Grades";
}

<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">Subject Grades List</h2>
        </div>
    </div>
    <br />
    <a asp-controller="Subject" asp-action="Index" class="btn btn-secondary" style="width:150px">
        Back to List
    </a>
    <table border="0" cellspacing="5" cellpadding="5">
        <tbody><tr>
            <td>Minimum date:</td>
            <td><input type="text" id="min" name="min"></td>
        </tr>
        <tr>
            <td>Maximum date:</td>
            <td><input type="text" id="max" name="max"></td>
        </tr>
    </tbody></table>
    <table  id="tblData"  class="table table-bordered table-striped" style="width:100%">
        <thead>
            <tr>
                <th>Student</th>
                <th>Mark</th>
                <th>Datetime</th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts{
    <script src="~/js/subjectGrades.js"></script>
}
